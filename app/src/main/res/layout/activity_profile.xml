<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background">

    <ImageButton
        android:id="@+id/editName"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="20dp"
        android:scaleType="fitCenter"
        android:src="@drawable/img"
        app:layout_constraintBottom_toTopOf="@+id/userName"
        app:layout_constraintStart_toEndOf="@+id/textView8"></ImageButton>

    <ImageButton
        android:id="@+id/editEmail"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="80dp"
        android:scaleType="fitCenter"
        android:src="@drawable/img"
        app:layout_constraintBottom_toTopOf="@+id/userEmail"
        app:layout_constraintStart_toEndOf="@+id/textView9"></ImageButton>

    <ImageButton
        android:id="@+id/editAge"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="100dp"
        android:scaleType="fitCenter"
        android:src="@drawable/img"
        app:layout_constraintBottom_toTopOf="@+id/userAge"
        app:layout_constraintStart_toEndOf="@+id/textView10"></ImageButton>


    <ImageButton
        android:id="@+id/editPhone"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="64dp"
        android:scaleType="fitCenter"
        android:src="@drawable/img"
        app:layout_constraintBottom_toTopOf="@+id/userPhone"
        app:layout_constraintStart_toEndOf="@+id/textView11"></ImageButton>

    <Button
        android:id="@+id/LogoutButton"
        android:layout_width="239dp"
        android:layout_height="62dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="#FFFFFF"
        android:fontFamily="sans-serif"
        android:text="LOGOUT"
        android:textColor="#000000"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userPhone" />

    <TextView
        android:id="@+id/Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="Profile Page"
        android:textColor="#FF9800"
        android:textSize="50sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:typeface="serif" />

    <TextView
        android:id="@+id/userName"
        android:layout_width="342dp"
        android:layout_height="42dp"
        android:layout_marginEnd="24dp"
        android:text="Full Name"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/userEmail"
        android:layout_width="396dp"
        android:layout_height="39dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="Email"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/userAge"
        android:layout_width="104dp"
        android:layout_height="40dp"
        android:layout_marginEnd="264dp"
        android:text="Age"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/userPhone"
        android:layout_width="341dp"
        android:layout_height="40dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="32dp"
        android:text="Phone Number"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="60dp"
        android:text="Full Name"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Profile" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="68dp"
        android:text="Email"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:textStyle="bold" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="60dp"
        android:text="Age"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        tools:textStyle="bold" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="93dp"
        android:layout_height="37dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="60dp"
        android:text="Phone"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:textStyle="bold" />

    <Button
        android:id="@+id/news"
        android:layout_width="104dp"
        android:layout_height="60dp"
        android:insetBottom="0dp"
        android:text="News"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/fav"
        android:layout_width="104dp"
        android:layout_height="60dp"
        android:insetBottom="0dp"
        android:text="Favs"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/news" />

    <Button
        android:id="@+id/home"
        android:layout_width="104dp"
        android:layout_height="60dp"
        android:insetBottom="0dp"
        android:text="Home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/fav" />

    <Button
        android:id="@+id/profile"
        android:layout_width="104dp"
        android:layout_height="60dp"
        android:insetBottom="0dp"
        android:text="Profile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/home" />

</androidx.constraintlayout.widget.ConstraintLayout>